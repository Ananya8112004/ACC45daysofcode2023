#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,a,b;
	    int c1,c2;
	    c1=0;
	    c2=0;
	    cin>>n>>a>>b;
	    string s;
	    cin>>s;
	    for(int i=0;i<n;i++){
	        if(s[i]=='0'){
	            c1++;
	        }
	        else{
	            c2++;
	        }
	    }
	    cout<<(c1*a)+(c2*b)<<endl;
	    
	}
	return 0;
}
